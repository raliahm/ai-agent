# Configuration for GPT-4 Local Client
# Copy this file to .env and update with your settings

# Local GPT-4 API URL (adjust to your setup)
GPT4_BASE_URL=http://localhost:8080

# API Key (if your local setup requires authentication)
# GPT4_API_KEY=your_api_key_here

# Default model name
GPT4_MODEL=gpt-4

# Default temperature for responses
GPT4_TEMPERATURE=0.7
